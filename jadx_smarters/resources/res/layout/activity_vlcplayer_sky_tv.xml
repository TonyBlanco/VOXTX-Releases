<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:keepScreenOn="true"
    android:layoutDirection="locale">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/dashboard_background"
        android:scaleType="fitXY"/>
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbar_toolbar"
        android:background="@color/transparent"
        android:fitsSystemWindows="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="-10dp">
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/ll_toolbar"
            android:paddingTop="20dp"
            android:paddingBottom="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingStart="10dp"
            android:paddingEnd="10dp">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/logo"
                    android:padding="4dp"
                    android:focusable="false"
                    android:layout_width="150dp"
                    android:layout_height="42dp"
                    android:src="@drawable/logo_white_long"/>
                <TextView
                    android:textSize="18sp"
                    android:textColor="@color/light_gray"
                    android:gravity="center"
                    android:id="@+id/separator"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginTop="4dp"
                    android:text="|"
                    android:textDirection="locale"
                    android:layout_toEndOf="@+id/logo"/>
                <TextView
                    android:textSize="16sp"
                    android:textColor="@color/light_gray"
                    android:gravity="center"
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginTop="8dp"
                    android:text="@string/live_sky"
                    android:textDirection="locale"
                    android:layout_marginStart="5dp"
                    android:layout_toEndOf="@+id/separator"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="50dp">
                <androidx.appcompat.widget.Toolbar
                    android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
                    android:id="@+id/toolbar"
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:minHeight="?attr/actionBarSize"
                    app:popupTheme="@style/ThemeOverlay.AppCompat.Light">
                    <RelativeLayout
                        android:id="@+id/rl_toolbar1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layoutDirection="locale">
                        <RelativeLayout
                            android:id="@+id/rl_settings"
                            android:visibility="visible"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layoutDirection="locale"
                            android:layout_marginStart="10dp"
                            android:layout_marginEnd="15dp">
                            <TextView
                                android:textSize="16sp"
                                android:textColor="@android:color/white"
                                android:gravity="center"
                                android:id="@+id/time"
                                android:visibility="visible"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginTop="8dp"
                                android:textDirection="locale"
                                android:layout_marginEnd="10dp"
                                android:layout_toStartOf="@+id/date"/>
                            <TextView
                                android:textSize="14sp"
                                android:textColor="#ffcccccc"
                                android:gravity="center"
                                android:id="@+id/date"
                                android:visibility="visible"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginTop="8dp"
                                android:textDirection="locale"
                                android:paddingEnd="30sp"
                                android:layout_alignParentEnd="true"/>
                        </RelativeLayout>
                    </RelativeLayout>
                </androidx.appcompat.widget.Toolbar>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/ll_layout_to_hide_1"
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:layoutDirection="locale"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/ll_layout_to_hide_1"
            android:layoutDirection="locale">
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:id="@+id/ll_layout_to_hide_4"
                android:tag="2"
                android:visibility="visible"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.6"
                android:layoutDirection="locale"
                android:layout_marginStart="0sp">
                <RelativeLayout
                    android:id="@+id/rl_episodes_box"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageView
                        android:visibility="visible"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"/>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:baselineAligned="false"
                        android:weightSum="2"
                        android:layout_marginStart="25dp"
                        android:layout_marginEnd="10dp">
                        <LinearLayout
                            android:id="@+id/rl_left"
                            android:background="@color/trasparent_light"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <RelativeLayout
                                    android:id="@+id/rl_header_sidebar"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <FrameLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content">
                                        <LinearLayout
                                            android:orientation="horizontal"
                                            android:background="@color/trasparent_light_2"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:baselineAligned="false"
                                            android:weightSum="10">
                                            <RelativeLayout
                                                android:id="@+id/rl_search_cat"
                                                android:tag="11"
                                                android:background="@color/cat_search_background_live"
                                                android:focusable="true"
                                                android:nextFocusRight="@+id/lv_ch_left_side"
                                                android:layout_width="match_parent"
                                                android:layout_height="40dp">
                                                <LinearLayout
                                                    android:layout_width="match_parent"
                                                    android:layout_height="match_parent">
                                                    <ImageView
                                                        android:layout_gravity="center"
                                                        android:id="@+id/iv_search_cat"
                                                        android:layout_width="20dp"
                                                        android:layout_height="20dp"
                                                        android:layout_marginLeft="10dp"
                                                        android:layout_marginTop="2dp"
                                                        android:layout_marginBottom="2dp"
                                                        android:src="@drawable/search_white"
                                                        android:layout_marginStart="10dp"/>
                                                    <EditText
                                                        android:textSize="14sp"
                                                        android:textColor="@color/white"
                                                        android:textColorHint="#ff918f8f"
                                                        android:gravity="center_vertical"
                                                        android:id="@+id/et_search_left_side"
                                                        android:background="@null"
                                                        android:layout_width="match_parent"
                                                        android:layout_height="match_parent"
                                                        android:layout_marginLeft="5dp"
                                                        android:hint="@string/search_in_categories"
                                                        android:singleLine="true"
                                                        android:layout_marginStart="5dp"/>
                                                </LinearLayout>
                                            </RelativeLayout>
                                        </LinearLayout>
                                    </FrameLayout>
                                </RelativeLayout>
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_below="@+id/rl_header_sidebar">
                                    <androidx.recyclerview.widget.RecyclerView
                                        android:id="@+id/recycler_view_left_sidebar"
                                        android:layout_width="0dp"
                                        android:layout_height="0dp"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"/>
                                </androidx.constraintlayout.widget.ConstraintLayout>
                                <LinearLayout
                                    android:id="@+id/ll_no_cat_found"
                                    android:visibility="gone"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_below="@+id/rl_header_sidebar">
                                    <TextView
                                        android:textColor="@color/white"
                                        android:gravity="top|center_horizontal"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginTop="20dp"
                                        android:text="@string/no_cat_found"/>
                                </LinearLayout>
                            </RelativeLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/rl_left_channel"
                            android:background="@color/trasparent_light"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">
                                    <ListView
                                        android:scrollbarThumbVertical="@drawable/scrollbar_list_view"
                                        android:scrollbarStyle="insideOverlay"
                                        android:id="@+id/lv_ch_left_side"
                                        android:focusable="true"
                                        android:visibility="gone"
                                        android:nextFocusRight="@+id/rl_nst_player_sky_layout"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:fadeScrollbars="false"/>
                                </LinearLayout>
                                <LinearLayout
                                    android:id="@+id/ll_no_left_channel_found"
                                    android:visibility="gone"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">
                                    <TextView
                                        android:textColor="@color/white"
                                        android:gravity="top|center_horizontal"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginTop="20dp"
                                        android:text="@string/no_channel_found"/>
                                </LinearLayout>
                                <LinearLayout
                                    android:id="@+id/ll_pb_left_channel_list"
                                    android:visibility="visible"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">
                                    <ProgressBar
                                        android:theme="@style/whiteLoader"
                                        android:id="@+id/pb_left_channel_list"
                                        android:visibility="visible"
                                        android:layout_width="match_parent"
                                        android:layout_height="40dp"
                                        android:layout_marginTop="25dp"
                                        android:indeterminateOnly="true"
                                        android:indeterminateBehavior="repeat"/>
                                </LinearLayout>
                            </RelativeLayout>
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:id="@+id/catch_up"
                android:tag="3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:weightSum="1"
                android:layout_weight="0.4"
                android:layoutDirection="locale"
                android:layout_marginStart="0sp">
                <RelativeLayout
                    android:id="@+id/rl_video_box"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.5"
                    android:layoutDirection="locale">
                    <RelativeLayout
                        android:id="@+id/rl_nst_player_sky_layout"
                        android:background="@drawable/selector_checkbox"
                        android:padding="4dp"
                        android:focusable="true"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layoutDirection="locale"
                        android:layout_toStartOf="@+id/rl_layout_to_hide_6">
                        <include
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            layout="@layout/honey_player_live_tv"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_layout_to_hide_6"
                        android:layout_width="20dp"
                        android:layout_height="match_parent"
                        android:layoutDirection="locale"
                        android:layout_alignParentEnd="true"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_layout_to_hide_5"
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.5"
                    android:layoutDirection="locale"
                    android:layout_marginEnd="20dp">
                    <ScrollView
                        android:id="@+id/scrollView"
                        android:background="@drawable/selector_checkbox"
                        android:focusable="true"
                        android:nextFocusDown="@+id/scrollView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="7dp"
                        android:layoutDirection="locale">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layoutDirection="locale">
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:id="@+id/ll_epg1_box"
                                    android:visibility="gone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layoutDirection="locale"
                                    android:layout_marginStart="5dp"
                                    android:layout_marginEnd="5dp">
                                    <LinearLayout
                                        android:gravity="center_vertical"
                                        android:orientation="horizontal"
                                        android:id="@+id/ll_epg1_date"
                                        android:tag="4"
                                        android:layout_width="match_parent"
                                        android:layout_height="0dp"
                                        android:layout_weight="0.4"
                                        android:layout_below="@+id/on_demand"
                                        android:layoutDirection="locale"
                                        android:layout_marginStart="0sp"
                                        android:layout_toEndOf="@+id/live_tv">
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/hp_cyan_light"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg1_date"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textDirection="locale"
                                            android:layout_marginStart="5dp"/>
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/hp_cyan_light"
                                            android:gravity="center_vertical"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:text=""
                                            android:textDirection="locale"/>
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/hp_cyan_light"
                                            android:ellipsize="marquee"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg1_program"
                                            android:fadingEdge="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:scrollHorizontally="true"
                                            android:singleLine="true"
                                            android:marqueeRepeatLimit="marquee_forever"
                                            android:textDirection="locale"
                                            android:layout_marginStart="10dp"/>
                                    </LinearLayout>
                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:id="@+id/ll_epg1_desc"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content">
                                        <TextView
                                            android:textSize="12sp"
                                            android:textColor="@color/white_trasparent"
                                            android:ellipsize="end"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg1_desc"
                                            android:fadingEdge="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:maxLines="2"
                                            android:textDirection="locale"
                                            android:layout_marginStart="5dp"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:id="@+id/ll_epg2_box"
                                    android:visibility="gone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:layoutDirection="locale"
                                    android:layout_marginStart="5dp"
                                    android:layout_marginEnd="5dp">
                                    <LinearLayout
                                        android:gravity="center_vertical"
                                        android:orientation="horizontal"
                                        android:id="@+id/ll_epg2_date"
                                        android:tag="4"
                                        android:layout_width="match_parent"
                                        android:layout_height="0dp"
                                        android:weightSum="1"
                                        android:layout_weight="0.4"
                                        android:layout_below="@+id/on_demand"
                                        android:layoutDirection="locale"
                                        android:layout_marginStart="0sp"
                                        android:layout_toEndOf="@+id/live_tv">
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/white"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg2_date"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textDirection="locale"
                                            android:layout_marginStart="5dp"/>
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/white"
                                            android:gravity="center_vertical"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textDirection="locale"/>
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/white"
                                            android:ellipsize="marquee"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg2_program"
                                            android:fadingEdge="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:scrollHorizontally="true"
                                            android:singleLine="true"
                                            android:marqueeRepeatLimit="marquee_forever"
                                            android:textDirection="locale"
                                            android:layout_marginStart="10sp"/>
                                    </LinearLayout>
                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:id="@+id/ll_epg2_desc"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content">
                                        <TextView
                                            android:textSize="12sp"
                                            android:textColor="@color/white_trasparent"
                                            android:ellipsize="end"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg2_desc"
                                            android:fadingEdge="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:maxLines="2"
                                            android:textDirection="locale"
                                            android:layout_marginStart="5dp"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:id="@+id/ll_epg3_box"
                                    android:visibility="gone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:layoutDirection="locale"
                                    android:layout_marginStart="5dp"
                                    android:layout_marginEnd="5dp">
                                    <LinearLayout
                                        android:gravity="center_vertical"
                                        android:orientation="horizontal"
                                        android:id="@+id/ll_epg3_date"
                                        android:tag="4"
                                        android:layout_width="match_parent"
                                        android:layout_height="0dp"
                                        android:weightSum="1"
                                        android:layout_weight="0.4"
                                        android:layout_below="@+id/on_demand"
                                        android:layoutDirection="locale"
                                        android:layout_marginStart="0sp"
                                        android:layout_toEndOf="@+id/live_tv">
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/white"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg3_date"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textDirection="locale"
                                            android:layout_marginStart="5dp"/>
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/white"
                                            android:gravity="center_vertical"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textDirection="locale"/>
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/white"
                                            android:ellipsize="marquee"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg3_program"
                                            android:fadingEdge="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:scrollHorizontally="true"
                                            android:singleLine="true"
                                            android:marqueeRepeatLimit="marquee_forever"
                                            android:textDirection="locale"
                                            android:layout_marginStart="10sp"/>
                                    </LinearLayout>
                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:id="@+id/ll_epg3_desc"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content">
                                        <TextView
                                            android:textSize="12sp"
                                            android:textColor="@color/white_trasparent"
                                            android:ellipsize="end"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg3_desc"
                                            android:fadingEdge="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:maxLines="2"
                                            android:textDirection="locale"
                                            android:layout_marginStart="5dp"/>
                                    </LinearLayout>
                                </LinearLayout>
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:id="@+id/ll_epg4_box"
                                    android:visibility="gone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:layoutDirection="locale"
                                    android:layout_marginStart="5dp"
                                    android:layout_marginEnd="5dp">
                                    <LinearLayout
                                        android:gravity="center_vertical"
                                        android:orientation="horizontal"
                                        android:id="@+id/ll_epg4_date"
                                        android:tag="4"
                                        android:layout_width="match_parent"
                                        android:layout_height="0dp"
                                        android:weightSum="1"
                                        android:layout_weight="0.4"
                                        android:layout_below="@+id/on_demand"
                                        android:layoutDirection="locale"
                                        android:layout_marginStart="0sp"
                                        android:layout_toEndOf="@+id/live_tv">
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/white"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg4_date"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textDirection="locale"
                                            android:layout_marginStart="5dp"/>
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/white"
                                            android:gravity="center_vertical"
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:textDirection="locale"/>
                                        <TextView
                                            android:textSize="15sp"
                                            android:textColor="@color/white"
                                            android:ellipsize="marquee"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg4_program"
                                            android:fadingEdge="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:scrollHorizontally="true"
                                            android:singleLine="true"
                                            android:marqueeRepeatLimit="marquee_forever"
                                            android:textDirection="locale"
                                            android:layout_marginStart="10sp"/>
                                    </LinearLayout>
                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:id="@+id/ll_epg4_desc"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content">
                                        <TextView
                                            android:textSize="12sp"
                                            android:textColor="@color/white_trasparent"
                                            android:ellipsize="end"
                                            android:gravity="center_vertical"
                                            android:id="@+id/tv_epg4_desc"
                                            android:fadingEdge="horizontal"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:maxLines="2"
                                            android:textDirection="locale"
                                            android:layout_marginStart="5dp"/>
                                    </LinearLayout>
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:orientation="vertical"
                                android:id="@+id/ll_no_guide"
                                android:paddingTop="20dp"
                                android:visibility="gone"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layoutDirection="locale">
                                <TextView
                                    android:textSize="14sp"
                                    android:textColor="@color/white_trasparent"
                                    android:gravity="center"
                                    android:id="@+id/text1"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="@string/tv_guide_info_downloading"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/ll_epg_loader"
                                android:visibility="gone"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textDirection="locale">
                                <ProgressBar
                                    android:theme="@style/blueLoader"
                                    android:layout_gravity="center"
                                    android:visibility="visible"
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:indeterminateOnly="true"
                                    android:indeterminateBehavior="repeat"/>
                            </LinearLayout>
                        </LinearLayout>
                    </ScrollView>
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </com.google.android.material.appbar.AppBarLayout>
</FrameLayout>
