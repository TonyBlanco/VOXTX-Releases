<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:keepScreenOn="true"
    android:layoutDirection="locale">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/dashboard_background"
        android:scaleType="fitXY"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layoutDirection="locale">
        <RelativeLayout
            android:id="@+id/rl_toolbar"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layoutDirection="locale">
            <com.google.android.material.appbar.AppBarLayout
                android:id="@+id/appbar_toolbar"
                android:background="@color/transparent"
                android:fitsSystemWindows="true"
                android:layout_width="match_parent"
                android:layout_height="50dp">
                <androidx.appcompat.widget.Toolbar
                    android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
                    android:id="@+id/toolbar"
                    android:visibility="visible"
                    android:fitsSystemWindows="true"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:minHeight="?attr/actionBarSize"
                    android:layout_centerInParent="true"
                    app:popupTheme="@style/ThemeOverlay.AppCompat.Light">
                    <RelativeLayout
                        android:id="@+id/rl_toolbar1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layoutDirection="locale">
                        <RelativeLayout
                            android:id="@+id/rl_settings"
                            android:visibility="visible"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:layoutDirection="locale"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="25dp">
                            <ImageView
                                android:id="@+id/logo"
                                android:focusable="false"
                                android:layout_width="150dp"
                                android:layout_height="42dp"
                                android:src="@drawable/logo_white_long"/>
                            <TextView
                                android:textSize="18sp"
                                android:textColor="@color/light_gray"
                                android:gravity="center"
                                android:id="@+id/separator"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginTop="4dp"
                                android:text="|"
                                android:textDirection="locale"
                                android:layout_toEndOf="@+id/logo"/>
                            <TextView
                                android:textSize="16sp"
                                android:textColor="@color/light_gray"
                                android:gravity="center"
                                android:id="@+id/textView"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginTop="8dp"
                                android:text="@string/live_sky"
                                android:textDirection="locale"
                                android:layout_marginStart="5dp"
                                android:layout_toEndOf="@+id/separator"/>
                            <TextView
                                android:textSize="16sp"
                                android:textColor="@android:color/white"
                                android:gravity="center"
                                android:id="@+id/time"
                                android:visibility="visible"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginTop="8dp"
                                android:textDirection="locale"
                                android:layout_marginEnd="10dp"
                                android:layout_toStartOf="@+id/date"/>
                            <TextView
                                android:textSize="14sp"
                                android:textColor="#ffcccccc"
                                android:gravity="center"
                                android:id="@+id/date"
                                android:visibility="visible"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginTop="8dp"
                                android:textDirection="locale"
                                android:paddingEnd="30sp"
                                android:layout_alignParentEnd="true"/>
                        </RelativeLayout>
                    </RelativeLayout>
                </androidx.appcompat.widget.Toolbar>
            </com.google.android.material.appbar.AppBarLayout>
        </RelativeLayout>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/ll_layout_to_hide_1"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:layout_below="@+id/rl_toolbar"
            android:layoutDirection="locale"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="4"
            android:layout_below="@+id/ll_layout_to_hide_1"
            android:layoutDirection="locale">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:layoutDirection="locale">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layoutDirection="locale">
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/ll_layout_to_hide_4"
                        android:tag="2"
                        android:focusable="true"
                        android:visibility="gone"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.6"
                        android:layoutDirection="locale"
                        android:layout_marginStart="0sp">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="7dp"
                            android:layoutDirection="locale">
                            <RelativeLayout
                                android:id="@+id/rl_categories_view"
                                android:visibility="visible"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layoutDirection="locale"
                                android:layout_marginStart="25dp"
                                android:layout_marginEnd="25dp"
                                android:layout_alignParentEnd="true">
                                <androidx.appcompat.widget.AppCompatImageView
                                    android:layout_gravity="center"
                                    android:id="@+id/btn_category_back"
                                    android:background="@color/live_sky"
                                    android:padding="5dp"
                                    android:layout_width="30dp"
                                    android:layout_height="50dp"
                                    android:foreground="?android:attr/selectableItemBackground"
                                    app:srcCompat="@drawable/left_icon_cat"/>
                                <TextView
                                    android:textSize="18sp"
                                    android:textStyle="bold"
                                    android:textColor="@color/live_sky_text"
                                    android:ellipsize="marquee"
                                    android:gravity="center"
                                    android:id="@+id/tv_categories_view"
                                    android:background="@color/live_sky"
                                    android:padding="10dp"
                                    android:visibility="visible"
                                    android:fadingEdge="horizontal"
                                    android:nextFocusDown="@+id/et_search"
                                    android:layout_width="match_parent"
                                    android:layout_height="50dp"
                                    android:scrollHorizontally="true"
                                    android:singleLine="true"
                                    android:layout_alignParentTop="true"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:textDirection="locale"
                                    android:textAlignment="center"
                                    android:layout_toStartOf="@+id/btn_category_fwd"
                                    android:layout_toEndOf="@+id/btn_category_back"/>
                                <androidx.appcompat.widget.AppCompatImageView
                                    android:layout_gravity="center"
                                    android:id="@+id/btn_category_fwd"
                                    android:background="@color/live_sky"
                                    android:padding="5dp"
                                    android:layout_width="30dp"
                                    android:layout_height="50dp"
                                    android:foreground="?android:attr/selectableItemBackground"
                                    android:layout_alignParentEnd="true"
                                    app:srcCompat="@drawable/right_icon_cat"/>
                            </RelativeLayout>
                            <EditText
                                android:textSize="18dp"
                                android:textStyle="bold"
                                android:textColor="@color/white"
                                android:textColorHint="@color/white"
                                android:id="@+id/et_search"
                                android:background="@drawable/selector_search_fields"
                                android:padding="10dp"
                                android:visibility="gone"
                                android:nextFocusDown="@+id/lv_ch"
                                android:layout_width="400dp"
                                android:layout_height="40dp"
                                android:layout_marginTop="2dp"
                                android:hint="Search"
                                android:layout_below="@+id/rl_categories_view"
                                android:imeOptions="flagNoExtractUi"
                                android:textDirection="locale"
                                android:layout_alignParentEnd="true"/>
                            <ListView
                                android:scrollbarThumbVertical="@drawable/scrollbar_list_view"
                                android:scrollbarStyle="insideOverlay"
                                android:id="@+id/lv_ch"
                                android:background="@color/live_sky"
                                android:focusable="false"
                                android:visibility="visible"
                                android:nextFocusUp="@+id/et_search"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:listSelector="@drawable/selector_sky_list"
                                android:layout_below="@+id/et_search"
                                android:fadeScrollbars="false"
                                android:layout_marginStart="25dp"
                                android:layout_marginEnd="25dp"
                                android:layout_alignParentEnd="false">
                                <requestFocus/>
                            </ListView>
                            <ProgressBar
                                android:theme="@style/blueLoader"
                                android:layout_gravity="center"
                                android:id="@+id/pb_listview_loader"
                                android:visibility="gone"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:layout_marginTop="25dp"
                                android:indeterminateOnly="true"
                                android:indeterminateBehavior="repeat"
                                android:layout_below="@+id/et_search"
                                android:layout_marginStart="25dp"
                                android:layout_marginEnd="25dp"/>
                            <TextView
                                android:textSize="18sp"
                                android:textColor="@color/white"
                                android:id="@+id/tv_no_record_found"
                                android:visibility="invisible"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/no_channel_found"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:textDirection="locale"/>
                        </RelativeLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/catch_up"
                        android:tag="3"
                        android:focusable="true"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.5"
                        android:layoutDirection="locale"
                        android:layout_marginStart="0sp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:tag="3"
                            android:focusable="true"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="0.5"
                            android:layout_marginStart="0sp">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:weightSum="1"
                                android:layoutDirection="locale">
                                <RelativeLayout
                                    android:id="@+id/rl_video_box"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="1"
                                    android:layoutDirection="locale">
                                    <RelativeLayout
                                        android:id="@+id/rl_nst_player_sky_layout"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layoutDirection="locale"
                                        android:layout_toStartOf="@+id/rl_layout_to_hide_6">
                                        <include
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            layout="@layout/nst_exo_player_vod"/>
                                    </RelativeLayout>
                                    <RelativeLayout
                                        android:id="@+id/rl_layout_to_hide_6"
                                        android:visibility="gone"
                                        android:layout_width="25dp"
                                        android:layout_height="match_parent"
                                        android:layoutDirection="locale"
                                        android:layout_alignParentEnd="true"/>
                                </RelativeLayout>
                                <RelativeLayout
                                    android:id="@+id/rl_layout_to_hide_5"
                                    android:visibility="gone"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="0.5"
                                    android:layoutDirection="locale"
                                    android:layout_marginEnd="25dp">
                                    <RelativeLayout
                                        android:background="#6a000000"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginTop="20dp"
                                        android:layout_marginBottom="7dp"
                                        android:layoutDirection="locale"
                                        android:layout_alignParentEnd="true">
                                        <LinearLayout
                                            android:orientation="horizontal"
                                            android:visibility="visible"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layoutDirection="locale">
                                            <LinearLayout
                                                android:orientation="vertical"
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent"
                                                android:weightSum="4"
                                                android:layoutDirection="locale">
                                                <LinearLayout
                                                    android:orientation="horizontal"
                                                    android:id="@+id/ll_epg1_box"
                                                    android:visibility="gone"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="0dp"
                                                    android:layout_weight="1"
                                                    android:layoutDirection="locale"
                                                    android:layout_marginStart="5dp"
                                                    android:layout_marginEnd="5dp">
                                                    <LinearLayout
                                                        android:gravity="center_vertical"
                                                        android:orientation="horizontal"
                                                        android:id="@+id/ll_epg1_date"
                                                        android:tag="4"
                                                        android:focusable="true"
                                                        android:layout_width="0dp"
                                                        android:layout_height="match_parent"
                                                        android:layout_weight="0.4"
                                                        android:layout_below="@+id/on_demand"
                                                        android:layoutDirection="locale"
                                                        android:layout_marginStart="0sp"
                                                        android:layout_toEndOf="@+id/live_tv">
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/colorPrimaryFocus"
                                                            android:gravity="center_vertical"
                                                            android:id="@+id/tv_epg1_date"
                                                            android:layout_width="wrap_content"
                                                            android:layout_height="match_parent"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="5dp"/>
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/colorPrimaryFocus"
                                                            android:gravity="center_vertical"
                                                            android:layout_width="wrap_content"
                                                            android:layout_height="match_parent"
                                                            android:text="=&gt;"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="10dp"/>
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/colorPrimaryFocus"
                                                            android:ellipsize="marquee"
                                                            android:gravity="center_vertical"
                                                            android:id="@+id/tv_epg1_program"
                                                            android:fadingEdge="horizontal"
                                                            android:layout_width="match_parent"
                                                            android:layout_height="match_parent"
                                                            android:scrollHorizontally="true"
                                                            android:singleLine="true"
                                                            android:marqueeRepeatLimit="marquee_forever"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="10dp"/>
                                                    </LinearLayout>
                                                </LinearLayout>
                                                <LinearLayout
                                                    android:orientation="horizontal"
                                                    android:id="@+id/ll_epg2_box"
                                                    android:visibility="gone"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="0dp"
                                                    android:layout_weight="1"
                                                    android:layoutDirection="locale"
                                                    android:layout_marginStart="5dp"
                                                    android:layout_marginEnd="5dp">
                                                    <LinearLayout
                                                        android:gravity="center_vertical"
                                                        android:orientation="horizontal"
                                                        android:id="@+id/ll_epg2_date"
                                                        android:tag="4"
                                                        android:focusable="true"
                                                        android:layout_width="0dp"
                                                        android:layout_height="match_parent"
                                                        android:weightSum="1"
                                                        android:layout_weight="0.4"
                                                        android:layout_below="@+id/on_demand"
                                                        android:layoutDirection="locale"
                                                        android:layout_marginStart="0sp"
                                                        android:layout_toEndOf="@+id/live_tv">
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/white"
                                                            android:gravity="center_vertical"
                                                            android:id="@+id/tv_epg2_date"
                                                            android:layout_width="wrap_content"
                                                            android:layout_height="match_parent"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="5dp"/>
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/white"
                                                            android:gravity="center_vertical"
                                                            android:layout_width="wrap_content"
                                                            android:layout_height="match_parent"
                                                            android:text="=&gt;"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="10sp"/>
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/white"
                                                            android:ellipsize="marquee"
                                                            android:gravity="center_vertical"
                                                            android:id="@+id/tv_epg2_program"
                                                            android:fadingEdge="horizontal"
                                                            android:layout_width="match_parent"
                                                            android:layout_height="match_parent"
                                                            android:scrollHorizontally="true"
                                                            android:singleLine="true"
                                                            android:marqueeRepeatLimit="marquee_forever"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="10sp"/>
                                                    </LinearLayout>
                                                </LinearLayout>
                                                <LinearLayout
                                                    android:orientation="horizontal"
                                                    android:id="@+id/ll_epg3_box"
                                                    android:visibility="gone"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="0dp"
                                                    android:layout_weight="1"
                                                    android:layoutDirection="locale"
                                                    android:layout_marginStart="5dp"
                                                    android:layout_marginEnd="5dp">
                                                    <LinearLayout
                                                        android:gravity="center_vertical"
                                                        android:orientation="horizontal"
                                                        android:id="@+id/ll_epg3_date"
                                                        android:tag="4"
                                                        android:focusable="true"
                                                        android:layout_width="0dp"
                                                        android:layout_height="match_parent"
                                                        android:weightSum="1"
                                                        android:layout_weight="0.4"
                                                        android:layout_below="@+id/on_demand"
                                                        android:layoutDirection="locale"
                                                        android:layout_marginStart="0sp"
                                                        android:layout_toEndOf="@+id/live_tv">
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/white"
                                                            android:gravity="center_vertical"
                                                            android:id="@+id/tv_epg3_date"
                                                            android:layout_width="wrap_content"
                                                            android:layout_height="match_parent"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="5dp"/>
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/white"
                                                            android:gravity="center_vertical"
                                                            android:layout_width="wrap_content"
                                                            android:layout_height="match_parent"
                                                            android:text="=&gt;"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="10sp"/>
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/white"
                                                            android:ellipsize="marquee"
                                                            android:gravity="center_vertical"
                                                            android:id="@+id/tv_epg3_program"
                                                            android:fadingEdge="horizontal"
                                                            android:layout_width="match_parent"
                                                            android:layout_height="match_parent"
                                                            android:scrollHorizontally="true"
                                                            android:singleLine="true"
                                                            android:marqueeRepeatLimit="marquee_forever"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="10sp"/>
                                                    </LinearLayout>
                                                </LinearLayout>
                                                <LinearLayout
                                                    android:orientation="horizontal"
                                                    android:id="@+id/ll_epg4_box"
                                                    android:visibility="gone"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="0dp"
                                                    android:layout_weight="1"
                                                    android:layoutDirection="locale"
                                                    android:layout_marginStart="5dp"
                                                    android:layout_marginEnd="5dp">
                                                    <LinearLayout
                                                        android:gravity="center_vertical"
                                                        android:orientation="horizontal"
                                                        android:id="@+id/ll_epg4_date"
                                                        android:tag="4"
                                                        android:focusable="true"
                                                        android:layout_width="0dp"
                                                        android:layout_height="match_parent"
                                                        android:weightSum="1"
                                                        android:layout_weight="0.4"
                                                        android:layout_below="@+id/on_demand"
                                                        android:layoutDirection="locale"
                                                        android:layout_marginStart="0sp"
                                                        android:layout_toEndOf="@+id/live_tv">
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/white"
                                                            android:gravity="center_vertical"
                                                            android:id="@+id/tv_epg4_date"
                                                            android:layout_width="wrap_content"
                                                            android:layout_height="match_parent"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="5dp"/>
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/white"
                                                            android:gravity="center_vertical"
                                                            android:layout_width="wrap_content"
                                                            android:layout_height="match_parent"
                                                            android:text="=&gt;"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="10sp"/>
                                                        <TextView
                                                            android:textSize="16sp"
                                                            android:textColor="@color/white"
                                                            android:ellipsize="marquee"
                                                            android:gravity="center_vertical"
                                                            android:id="@+id/tv_epg4_program"
                                                            android:fadingEdge="horizontal"
                                                            android:layout_width="match_parent"
                                                            android:layout_height="match_parent"
                                                            android:scrollHorizontally="true"
                                                            android:singleLine="true"
                                                            android:marqueeRepeatLimit="marquee_forever"
                                                            android:textDirection="locale"
                                                            android:layout_marginStart="10sp"/>
                                                    </LinearLayout>
                                                </LinearLayout>
                                                <LinearLayout
                                                    android:orientation="horizontal"
                                                    android:id="@+id/ll_no_guide"
                                                    android:visibility="gone"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="match_parent"
                                                    android:layoutDirection="locale">
                                                    <TextView
                                                        android:textSize="12sp"
                                                        android:textColor="@color/light_gray"
                                                        android:gravity="center"
                                                        android:id="@+id/text1"
                                                        android:layout_width="match_parent"
                                                        android:layout_height="match_parent"
                                                        android:text="@string/install_epg"/>
                                                </LinearLayout>
                                                <LinearLayout
                                                    android:id="@+id/ll_epg_loader"
                                                    android:visibility="gone"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="match_parent"
                                                    android:textDirection="locale">
                                                    <ProgressBar
                                                        android:theme="@style/blueLoader"
                                                        android:layout_gravity="center"
                                                        android:visibility="visible"
                                                        android:layout_width="match_parent"
                                                        android:layout_height="40dp"
                                                        android:indeterminateOnly="true"
                                                        android:indeterminateBehavior="repeat"/>
                                                </LinearLayout>
                                            </LinearLayout>
                                        </LinearLayout>
                                    </RelativeLayout>
                                </RelativeLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <RelativeLayout
                android:id="@+id/rl_footer_info"
                android:background="#6a000000"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="35sp"
                android:textAlignment="center"
                android:layout_marginStart="25dp"
                android:layout_marginEnd="25dp">
                <RelativeLayout
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:layoutDirection="locale">
                    <TextView
                        android:textSize="12sp"
                        android:textColor="@color/white"
                        android:gravity="center_vertical"
                        android:id="@+id/tv_list_options"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/long_press_to_show_more"
                        android:drawablePadding="10dp"
                        android:textDirection="locale"/>
                    <TextView
                        android:textSize="20sp"
                        android:textColor="@color/white"
                        android:id="@+id/tv_horizontal_line"
                        android:paddingBottom="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="|"
                        android:layout_centerVertical="true"
                        android:textDirection="locale"
                        android:layout_marginStart="20dp"
                        android:layout_toEndOf="@+id/tv_list_options"/>
                    <ImageView
                        android:id="@+id/iv_list_white"
                        android:layout_width="12dp"
                        android:layout_height="15dp"
                        android:src="@drawable/list_white"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="20dp"
                        android:layout_toEndOf="@+id/tv_horizontal_line"/>
                    <TextView
                        android:textSize="12sp"
                        android:textColor="@color/white"
                        android:gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/to_show_menu_option"
                        android:textDirection="locale"
                        android:textAlignment="center"
                        android:layout_marginStart="5dp"
                        android:layout_toEndOf="@+id/iv_list_white"/>
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
</FrameLayout>
